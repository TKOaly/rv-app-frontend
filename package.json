{
  "name": "rv-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "connected-react-router": "^6.4.0",
    "history": "^4.9.0",
    "node-sass": "^4.12.0",
    "node-uuid": "^1.4.8",
    "prop-types": "^15.6.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-flexbox-grid": "^2.0.0",
    "react-fontawesome": "^1.6.1",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^3.0.1",
    "react-transition-group": "^2.2.1",
    "redux": "^4.0.0",
    "redux-form": "^8.1.0",
    "redux-thunk": "^2.2.0",
    "validator": "^11.1.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint-js": "eslint 'src/**/*.{js,jsx}'",
    "fix-js": "eslint --fix 'src/**/*.{js,jsx}'",
    "build": "react-scripts build",
    "lint-scss": "stylelint src/**/*.scss --syntax=scss",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^5.1.9",
    "@storybook/addon-actions": "^5.1.8",
    "@storybook/addon-backgrounds": "^5.1.8",
    "@storybook/addon-knobs": "^5.1.8",
    "@storybook/addon-links": "^5.1.8",
    "@storybook/addon-storysource": "^5.1.8",
    "@storybook/addon-viewport": "^5.1.9",
    "@storybook/addons": "^5.1.8",
    "@storybook/react": "^5.1.8",
    "cypress": "^3.3.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.4.0",
    "husky": "^3.0.0",
    "lint-staged": "^9.0.2",
    "prettier-stylelint": "^0.4.2",
    "react-test-renderer": "^16.2.0",
    "redux-devtools-extension": "^2.13.7",
    "redux-mock-store": "^1.5.1",
    "sinon": "^7.3.2",
    "start-server-and-test": "^1.9.1",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-scss": "^3.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint-js",
      "npm run fix-js",
      "git add"
    ],
    "*.scss": [
      "npm run lint-scss"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx}",
      "src/reducers/**/*.{js,jsx}",
      "src/services/**/*.{js,jsx}",
      "src/*.{js,jsx}"
    ],
    "coverageReporters": [
      "lcov"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
