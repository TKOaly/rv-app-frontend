{
  "name": "rv-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.17.1",
    "node-sass": "^4.11.0",
    "node-uuid": "^1.4.8",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-flexbox-grid": "^2.0.0",
    "react-fontawesome": "^1.6.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "2.1.2",
    "react-transition-group": "^2.2.1",
    "redux": "^4.0.0",
    "redux-form": "^7.3.0",
    "redux-thunk": "^2.2.0",
    "validator": "^10.1.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint-js": "eslint 'src/**/*.{js,jsx}'",
    "fix-js": "eslint --fix src/**/*.{js,jsx}'",
    "build": "react-scripts build",
    "lint-css": "stylelint src/**/*.scss --syntax=scss",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "styleguide": "styleguidist server",
    "stylehuide:build": "styleguidist build"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.10",
    "@storybook/addon-links": "^3.3.10",
    "@storybook/addons": "^3.3.10",
    "@storybook/react": "^3.3.10",
    "babel-eslint": "9.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "5.6.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.3.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "prettier-stylelint": "^0.4.2",
    "react-styleguidist": "^6.2.2",
    "react-test-renderer": "^16.2.0",
    "redux-devtools-extension": "^2.13.7",
    "redux-mock-store": "^1.5.1",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-scss": "^2.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-js && yarn lint-css"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx}",
      "src/reducers/**/*.{js,jsx}",
      "src/services/**/*.{js,jsx}",
      "src/*.{js,jsx}"
    ],
    "coverageReporters": [
      "lcov"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
